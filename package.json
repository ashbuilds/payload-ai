{
  "name": "@ashbuilds/payload-ai",
  "version": "1.0.0",
  "private": false,
  "bugs": "https://github.com/ashbuilds/payload-plugins/issues",
  "repository": "https://github.com/ashbuilds/payload-plugins",
  "license": "MIT",
  "author": "ashbuilds",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm copyfiles && pnpm build:types && pnpm build:swc",
    "build:swc": "swc ./src -d ./dist --config-file .swcrc --strip-leading-paths",
    "build:dev": "pnpm build --watch",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png,json}\" dist/",
    "lint": "eslint src",
    "lint:fix": "eslint --fix --ext .ts,.tsx src",
    "prepublishOnly": "pnpm clean && pnpm build"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^0.0.30",
    "@ai-sdk/openai": "^0.0.34",
    "@langchain/core": "^0.2.15",
    "@langchain/openai": "^0.2.1",
    "ai": "^3.2.16",
    "dot-object": "^2.1.5",
    "elevenlabs": "^0.8.2",
    "handlebars": "4.7.8",
    "langchain": "^0.2.9",
    "openai": "^4.47.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@swc/cli": "0.3.12",
    "@types/dot-object": "2.1.6",
    "@types/react": "npm:types-react@19.0.0-beta.2",
    "@types/react-dom": "npm:types-react-dom@19.0.0-beta.2",
    "chokidar": "^3.6.0",
    "copyfiles": "2.4.1",
    "payload": "3.0.0-beta.65",
    "prettier": "3.3.2",
    "react": "19.0.0-rc-f994737d14-20240522",
    "react-dom": "19.0.0-rc-f994737d14-20240522",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "@payloadcms/next": "3.0.0-beta.65",
    "@payloadcms/richtext-lexical": "3.0.0-beta.65",
    "@payloadcms/translations": "3.0.0-beta.65",
    "@payloadcms/ui": "3.0.0-beta.65",
    "payload": "3.0.0-beta.65"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "require": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    },
    "main": "./dist/index.js",
    "registry": "https://registry.npmjs.org/",
    "types": "./dist/index.d.ts"
  }
}

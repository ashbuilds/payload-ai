{
  "name": "@ashbuilds/payload-ai",
  "version": "1.0.0",
  "private": false,
  "bugs": "https://github.com/ashbuilds/payload-plugins/issues",
  "repository": "https://github.com/ashbuilds/payload-plugins",
  "license": "MIT",
  "author": "ashbuilds",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm build:types && pnpm build:swc",
    "build:swc": "swc ./src -d ./dist --config-file .swcrc --strip-leading-paths --copy-files",
    "build:dev": "pnpm build --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "prepublishOnly": "pnpm clean && pnpm build"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^0.0.30",
    "@ai-sdk/openai": "^0.0.34",
    "@langchain/core": "^0.2.15",
    "@langchain/openai": "^0.2.1",
    "ai": "^3.2.16",
    "dot-object": "^2.1.5",
    "elevenlabs": "^0.8.2",
    "handlebars": "4.7.8",
    "langchain": "^0.2.9",
    "openai": "^4.47.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@swc/cli": "0.4.0",
    "@types/dot-object": "2.1.6",
    "@types/react": "npm:types-react@19.0.0-beta.2",
    "@types/react-dom": "npm:types-react-dom@19.0.0-beta.2",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "chokidar": "^3.6.0",
    "copyfiles": "2.4.1",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-x": "^3.0.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-perfectionist": "^2.11.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-regexp": "^2.6.0",
    "globals": "^15.8.0",
    "payload": "3.0.0-beta.65",
    "prettier": "^3.3.3",
    "react": "19.0.0-rc-f994737d14-20240522",
    "react-dom": "19.0.0-rc-f994737d14-20240522",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.1"
  },
  "peerDependencies": {
    "@payloadcms/next": "3.0.0-beta.65",
    "@payloadcms/richtext-lexical": "3.0.0-beta.65",
    "@payloadcms/translations": "3.0.0-beta.65",
    "@payloadcms/ui": "3.0.0-beta.65",
    "payload": "3.0.0-beta.65"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "require": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    },
    "main": "./dist/index.js",
    "registry": "https://registry.npmjs.org/",
    "types": "./dist/index.d.ts"
  }
}
